s="footer">						<br />						<p>							Created with							<a href="http://membrane-soa.org/soa-model/?ana">Membrane SOA Model</a>							<br />							Copyright Â© 2008-2013							<a href="http://www.predic8.com/">predic8 GmbH</a>							<br />							Email:							<a href="mailto:info@predic8.com">info@predic8.com</a>						</p>						<br />					</div>				</div>			</body>		</html>	</xsl:template>	<xsl:template match="Diffs">		<xsl:choose>			<xsl:when test="not(./Diff)">				<h2>There are no differences.</h2>			</xsl:when>			<xsl:otherwise>				<div>					<h2>Differences:</h2>					<xsl:if test="//@breaks = 'true'">						<div class="notice">							<img src="web/images/lightning.png" />							<span>The changes break the interface.</span>						</div>					</xsl:if>					<div>						<ul id="diffs" class="treeview">							<xsl:apply-templates select="Diff" />						</ul>					</div>				</div>				<div class="legend">					<p>These symbols indicate that:</p>					<ul style="list-style-type: none;">						<li>							<img src="web/images/add.png" />							: Element has been added to the schema.						</li>						<li>							<img src="web/images/remove.png" />							: Element has been removed from the schema.						</li>						<li>							<img src="web/images/tick.png" />							: The change will not influence the interface.						</li>						<li>							<img src="web/images/lightning.png" />							: The change will invalidate the interface.						</li>					</ul>				</div>			</xsl:otherwise>		</xsl:choose>	</xsl:template>	<xsl:template match="Diff">		<ul>			<li>				<div>					<xsl:if test="@safe = 'true'">						<img src="web/images/tick.png" title="This change will not influence the interface"/>					</xsl:if>					<xsl:if test="@breaks = 'true'">						<img src="web/images/lightning.png" title="This change invalidate the original interface"/>					</xsl:if>					<xsl:if test="contains(Description, 'added')">						<img src="web/images/add.png" title="This element has been added to the modified Schema"/>					</xsl:if>					<xsl:if test="contains(Description, 'removed')">						<img src="web/images/remove.png" title="This element has been removed from the original schema"/>					</xsl:if>					<xsl:value-of select="Description" />				</div>				<xsl:apply-templates select="Diff" />			</li>		</ul>	</xsl:template></xsl:stylesheet>